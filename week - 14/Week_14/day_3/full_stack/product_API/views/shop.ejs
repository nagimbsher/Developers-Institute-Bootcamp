<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%- include(`partials/headers`) %>  
    <h1>Shop</h1>
    
    <h2><%= locals.first_name%></h2>
    <h3><%= locals.last_name%></h3>
   

    <!-- if else Statement  -->
    <% if(locals.login) { %>
        <h2>Welcome</h2>
      <% } else { %>
        <h2>Please Login</h2>
      <% } %>
  
      <%
        let str = ''
        if(locals.login){
          str = '<h2>Welcome</h2>'
        }
        else{
          str = '<h2>Please Login</h2>'
        }
      %>
      <%- str %>


      <% for(const elem in users) { %>
        <h2> <%= users[elem].name%> </h2>
        <h2> <%= users[elem].username%> </h2>

      <% } %>
      
      
  
</body>
</html>